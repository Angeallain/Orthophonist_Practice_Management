<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane fx:id="parent" prefHeight="700.0" prefWidth="1000.0" styleClass="anchor-pane-with-color" stylesheets="@Styles.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.club.Controller.DotsController">
    <children>
      <ImageView fx:id="welcomePic" fitHeight="93.0" fitWidth="84.0" layoutX="111.0" layoutY="250.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@pics/icon.png" />
         </image>
      </ImageView>
        <Label fx:id="welcomeLabel" layoutX="215.0" layoutY="255.0" text="CABINET D'ORTHOPHONIE" textFill="WHITE">
            <font>
                <Font name="Segoe UI Black" size="50.0" />
            </font>
        </Label>


        <HBox fx:id="dotsPane" alignment="CENTER" layoutX="450.0" layoutY="379.0" spacing="20">
            <Circle fx:id="dot1" fill="BLUE" radius="10" />
            <Circle fx:id="dot2" fill="RED" radius="10" />
            <Circle fx:id="dot3" fill="GREEN" radius="10" />
        </HBox>




    </children>
</AnchorPane>
